            <!--<TextView-->
            <!--android:id="@+id/registration_detail_frg_txt_status"-->
            <!--android:layout_width="wrap_content"-->
            <!--android:layout_height="wrap_content"-->
            <!--android:layout_marginLeft="@dimen/layout_padding_ls"-->
